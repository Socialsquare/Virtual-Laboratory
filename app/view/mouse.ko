<div class="mouse-view">

    <div class="video-container" data-bind="template: { name: 'video', data: videoController }"></div>

    <div class="water-bottle" data-bind="drag: { item: bottle, offset: { left: 84, top: 182 } }"></div>

    <div class="dead-mouse" data-bind="visible: !mouse().alive()"></div>

    <div class="mouse" data-bind="drop: { handler: handleDropOnMouse, accept: DragHelper.acceptedByMouse }"></div>

    <div class="mousegraph" data-bind="plot: plotData"></div>

</div>
